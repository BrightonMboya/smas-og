{
    "name": "smasapp",
    "version": "3.0.0",
    "description": "generated by backend api generator",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon src/index.ts",
        "docker": "node dist/index.js",
        "prod": "sudo tsc -p .",
        "build": "sudo npm install && sudo npm run prod && sudo npm run start",
        "rebuild": "sudo npm install && sudo npm run prod && sudo npm run restart",
        "start": "sudo pm2 start dist/index.js --name smasapp && sudo pm2 save",
        "restart": "sudo pm2 restart smasapp && sudo pm2 save",
        "stop": "sudo pm2 delete smasapp && sudo pm2 save"
    },
    "keywords": [
        "backend api",
        "backend api generator",
        "api generator",
        "backend generetor",
        "bapig"
    ],
    "author": "hekima.dev",
    "license": "ISC",
    "dependencies": {
        "axios": "^1.2.0",
        "bapig": "^2.1.9",
        "cors": "^2.8.5",
        "express-fileupload": "^1.4.0",
        "fast-web-kit": "^0.6.7",
        "mongoose": "^7.4.5",
        "mongoose-autopopulate": "^1.0.1",
        "numeral": "^2.0.6",
        "pluralize": "^8.0.0"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/express-fileupload": "^1.4.1",
        "@types/mongoose": "^5.11.97",
        "@types/numeral": "^2.0.2",
        "@types/pluralize": "^0.0.29",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    }
}
